{
  "name": "hive-mind",
  "version": "2.0.0",
  "description": "We play boggle competitively",
  "engines": {
    "node": ">= 7.0.0"
  },
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "android": "expo start --android",
    "dev": "npm run start-server & npm run start-client",
    "eject": "expo eject",
    "ios": "expo start --ios",
    "lint": "eslint ./ --ignore-path .gitignore",
    "lint-fix": "npm run lint -- --fix",
    "seed": "node script/seed.js",
    "start": "node server",
    "start-client": "expo start",
    "start-server": "nodemon server -e html,js,scss --ignore public --ignore client",
    "test": "npm run test-server & npm run test-client",
    "test-client": "jest ./client/**/*.spec.js",
    "test-server": "NODE_ENV='test' mocha \"./server/**/*.spec.js\" \"./script/**/*.spec.js\" --require @babel/polyfill --require @babel/register",
    "web": "expo start --web",
    "kill-server": "lsof -ti:8080 | xargs kill"
  },
  "jest": {
    "preset": "react-native"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "npm run lint-fix",
      "git add"
    ],
    "*.{css,scss,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@react-native-community/masked-view": "^0.1.7",
    "@react-navigation/bottom-tabs": "^5.2.2",
    "@react-navigation/native": "^5.1.1",
    "@react-navigation/stack": "^5.2.2",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.9.5",
    "axios": "^0.19.2",
    "colors": "^1.4.0",
    "connect-session-sequelize": "^6.1.1",
    "crypto": "^1.0.1",
    "d3": "^5.15.0",
    "expo": "~36.0.0",
    "expo-constants": "^8.0.0",
    "expo-facebook": "^8.0.0",
    "expo-font": "^8.0.0",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "morgan": "^1.9.1",
    "native-base": "2.13.8",
    "passport": "^0.4.1",
    "path": "^0.12.7",
    "pg": "^7.18.2",
    "pg-hstore": "^2.3.2",
    "react": "~16.9.0",
    "react-devtools": "^4.5.0",
    "react-dom": "~16.9.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-36.0.0.tar.gz",
    "react-native-easy-grid": "^0.2.2",
    "react-native-gesture-handler": "^1.6.0",
    "react-native-reanimated": "^1.7.0",
    "react-native-safe-area-context": "^0.7.3",
    "react-native-screens": "^2.3.0",
    "react-native-svg": "^9.13.3",
    "react-native-web": "~0.11.7",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "sequelize": "^5.21.5",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "7.4.0",
    "@babel/plugin-proposal-decorators": "7.4.0",
    "@babel/plugin-proposal-export-namespace-from": "7.2.0",
    "@babel/plugin-proposal-function-sent": "7.2.0",
    "@babel/plugin-proposal-json-strings": "7.2.0",
    "@babel/plugin-proposal-numeric-separator": "7.2.0",
    "@babel/plugin-proposal-throw-expressions": "7.2.0",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-syntax-import-meta": "7.2.0",
    "@babel/polyfill": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.0",
    "axios-mock-adapter": "^1.16.0",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^8.0.5",
    "babel-preset-expo": "~8.0.0",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-config-fullstack": "^6.0.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-react": "^7.12.4",
    "husky": "^1.3.1",
    "jest": "^25.1.0",
    "lint-staged": "^8.1.5",
    "mocha": "^6.1.4",
    "npm-merge-driver": "^2.3.5",
    "prettier": "^1.19.1",
    "simple-git": "^1.121.0",
    "supertest": "^4.0.2",
    "yaml": "^1.6.0"
  },
  "private": true
}
